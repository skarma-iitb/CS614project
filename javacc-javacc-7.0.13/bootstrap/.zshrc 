
# >>> conda initialize >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/Users/skaryang/miniforge3/bin/conda' 'shell.zsh' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/Users/skaryang/miniforge3/etc/profile.d/conda.sh" ]; then
        . "/Users/skaryang/miniforge3/etc/profile.d/conda.sh"
    else
        export PATH="/Users/skaryang/miniforge3/bin:$PATH"
    fi
fi
export PATH="/Library/Frameworks/Python.framework/Versions/3.11/bin:$PATH"

unset __conda_setup
# <<< conda initialize <<<


export PATH="$PATH:/opt/homebrew/bin/npm"
export PATH="
export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"  # This loads nvm
[ -s "$NVM_DIR/bash_completion" ] && \. "$NVM_DIR/bash_completion"  # This loads nvm bash_completion
